package chap04_controlstatement;

public class _04_While {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 1. 1 ~ 100까지의 합
		int num = 1;
		int sum = 0;
		
		while(num <= 100)
		{
			sum += num++;
		}
		
		System.out.println("1~100까지의 합: " + sum);
		
		
		

	}

}
